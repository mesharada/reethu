pipeline { 
    agent any 
    tools { 
        maven 'Maven' // Ensure name matches with configured tool name exactly
    } 
    stages { 
        stage('Checkout') { 
            steps { 
                git branch: 'master', url: 'https://github.com/mesharada/reethu.git'
            } 
        } 
        stage('Build') { 
            steps { 
                bat 'mvn clean package' 
            } 
        } 
        stage('Test') { 
            steps { 
                bat 'mvn test' 
            } 
        } 
       stages('Run Application'){
           steps{
               bat 'java-jar target/reethu-0.0.1-SNAPSHOT.jar'
           }
       }
    } // <- closes stages
} // <- closes pipeline
